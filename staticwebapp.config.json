{
  "routes": [
    {
      "route": "/application/*",
      "serve": "/application/index.html",
      "statusCode": 200
    },
    {
      "route": "/application",
      "redirect": "/application/"
    },
    {
      "route": "/admin/*",
      "serve": "/admin/index.html",
      "statusCode": 200
    },
    {
      "route": "/admin",
      "redirect": "/admin/"
    },
    {
      "route": "/PDF_DL.pdf",
      "serve": "/PDF_DL.pdf",
      "headers": {
        "Content-Type": "application/pdf",
        "Content-Disposition": "attachment; filename=\"PDF_DL.pdf\"",
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/application/*",
      "/admin/*",
      "/images/*.{png,jpg,gif,svg}", 
      "/css/*", 
      "/js/*", 
      "/api/*", 
      "/*.pdf"
    ]
  },
  "mimeTypes": {
    ".pdf": "application/pdf",
    ".json": "application/json",
    ".css": "text/css",
    ".js": "application/javascript",
    ".html": "text/html",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".gif": "image/gif"
  }
}